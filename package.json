{
  "name": "next-js-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:prod": "env-cmd -f environments/.env.production next build && next export",
    "build:qa": "env-cmd -f environments/.env.qa next build && next export",
    "build:develop": "env-cmd -f environments/.env.development next build && next export",
    "start:prod": "env-cmd -f environments/.env.production next start",
    "start:qa": "env-cmd -f environments/.env.qa next dev",
    "start:dev": "env-cmd -f environments/.env.development next dev",
    "dev": "next dev",
    "custom:dev": "env-cmd -f environments/.env.development node server/index.js",
    "build": "next build && next export",
    "start": "next start",
    "test": "jest --coverage",
    "test:ci": "jest --ci",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint **/*.js",
    "lint:eslint": "eslint --ignore-path .eslintignore ",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "test:clean": "rimraf ./coverage",
    "test:staged": "jest --findRelatedTests",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "predeploy": "yarn build:prod",
    "deploy": "gh-pages -d out"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force",
      "jest --findRelatedTests $STAGED_FILES"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@formatjs/cli": "^4.2.27",
    "@formatjs/intl-relativetimeformat": "^9.1.7",
    "@formatjs/intl-utils": "^3.8.4",
    "@webcomponents/shadydom": "^1.8.0",
    "antd": "^4.9.3",
    "apisauce": "^2.1.1",
    "chalk": "4.1.1",
    "crypto-js": "^4.0.0",
    "env-cmd": "^10.1.0",
    "hoist-non-react-statics": "^3.3.2",
    "https": "^1.0.0",
    "immer": "^9.0.5",
    "immutable": "^4.0.0-rc.12",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.8.12",
    "invariant": "^2.2.4",
    "ip": "1.1.5",
    "lodash": "^4.17.21",
    "next": "^11.0.1",
    "next-images": "^1.8.1",
    "next-redux-wrapper": "^7.0.2",
    "object-hash": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^5.20.4",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-injectors": "^1.3.0",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.2.1",
    "reselect": "^4.0.0",
    "styled-components": "^5.3.0",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "12.0.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.13.1",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint-plugin-redux-saga": "^1.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.0.0",
    "next-transpile-modules": "^8.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "prettier-config-standard": "^4.0.0",
    "prettier-standard": "^16.4.1",
    "raf": "^3.4.1",
    "react-test-renderer": "^17.0.2",
    "react-testing-library": "^8.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
